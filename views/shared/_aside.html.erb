<!-- BEGIN shared/_aside.html.erb -->
<aside class="global">
	<% if @page.nil? == false %>
	<%= render :partial => '/shared/subNav' %>
	<% elsif @posts.nil? == false || @post.nil? == false %>
	<%= render :partial => '/shared/subNav_news' %>
	<% elsif @profiles.nil? == false %>
	<%= render :partial => '/shared/subNav_news' %>
	<% else %>
	<%= render :partial => '/shared/subNav_news' %>
	<% end %>
</aside>
<!-- END shared/_aside.html.erb -->