<!DOCTYPE html>
<html lang="en">
<head>
	<!-- LAYOUT profiles.html.erb -->
	<title>Profiles</title>
	<%= meta_tags(:html5 => true) %>
	<%= render :partial => '/shared/stylesMeta' %>
</head>
<body class="<%= @current_action %> <%= @current_controller %>">
