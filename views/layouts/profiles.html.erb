		<%= render :partial => '/shared/header' %>

		<section class="global">
			<%= yield %>
		</section>

		<%= render :partial => '/shared/aside' %>

		<%= render :partial => '/shared/footer' %>

		<%= render :partial => '/shared/scripts' %>
	</div><!-- end #wrapper -->
</body>
</html>